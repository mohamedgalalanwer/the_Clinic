        <activity
            android:name="com.example.projectos2.Book_AhmedReda"
            android:label="الدكتورأحمد رضا "
            android:theme="@android:style/Theme.Black" >
        </activity>
        <activity
            android:name="com.example.projectos2.Book_IbrahimElsayed"
            android:label=" الدكتور ابراهيم السيد "
            android:theme="@android:style/Theme.Black" >
        </activity>
        <activity
            android:name="com.example.projectos2.Book_RedaMahmoud"
             android:label=" الدكتور رضا محمود"
            android:theme="@android:style/Theme.Black"  >
        </activity>
    </application>

</manifest>
